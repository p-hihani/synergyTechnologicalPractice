library DelphiWebApp;

uses
  Winapi.Windows,
  System.Win.ComObj,
  System.SysUtils,
  Web.WebBroker,
  Web.Win.ISAPIApp,
  WebModuleUnit1 in 'WebModuleUnit1.pas' {WebModule1: TWebModule};

